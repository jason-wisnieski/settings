[user]
	name = Jeff Bencin
	email = jeff.bencin@telosalliance.com
[core]
	editor = vim
[alias]
	st      = status -uno
	ci      = commit
	br      = branch
	co      = checkout
	lg      = log --graph --date=short --pretty=format:'%C(green)%h %C(magenta)(%ar)%C(yellow)%d %C(cyan)%an: %C(reset)%s' 
	hist    = log --graph --date=short --pretty=format:'%C(green)%h %C(magenta)(%ar)%C(yellow)%d %C(cyan)%an: %C(reset)%s' --all
	tree    = forest --pretty=format:'%C(green)%h %C(magenta)(%ar) %C(cyan)%an: %C(reset)%s'
	trees   = forest --pretty=format:'%C(green)%h %C(magenta)(%ar) %C(cyan)%an: %C(reset)%s' --all
	subinit = submodule update --init --recursive
	maxgc   = gc --prune=now --aggressive
[color]
	ui = true
